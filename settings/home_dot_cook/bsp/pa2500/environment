ENV_OPENOCD_ROOT=/usr/local/share/openocd
ENV_OPENOCD_INTERFACE=stlink
ENV_CORTEX_VER=v7e-m/fpv4-sp
ENV_GCC_ROOT=/usr/bin
ENV_GCC_PREFIX=$(ENV_GCC_ROOT)/arm-none-eabi-
ENV_GCC_LIB=/../lib/gcc/arm-none-eabi/7.3.1
ENV_LIB_GCC=$(ENV_GCC_ROOT)$(ENV_GCC_LIB)/thumb/$(ENV_CORTEX_VER)/softfp/libgcc.a
ENV_LIB_M=$(ENV_GCC_ROOT)$(ENV_GCC_LIB)/../../../../lib/arm-none-eabi/newlib/thumb/$(ENV_CORTEX_VER)/softfp/libm.a
ENV_LIB_GPP=$(ENV_GCC_ROOT)$(ENV_GCC_LIB)/../../../../lib/arm-none-eabi/newlib/thumb/$(ENV_CORTEX_VER)/softfp/libstdc++_nano.a
